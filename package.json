{
  "name": "eliza-workspace",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "update-core": "cd packages/eliza-core && git pull origin main",
    "update-stable": "cd packages/eliza-stable && git fetch origin --tags && git checkout $(git describe --tags `git rev-list --tags --max-count=1`)"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=22.6.0"
  },
  "pnpm": {
    "overrides": {
      "@ai16z/core": "workspace:*",
      "@ai16z/agent": "workspace:*",
      "@ai16z/adapters": "workspace:*"
    }
  }
}